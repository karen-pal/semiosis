MAIN -> SENTENCE "."
SENTENCE -> SUB _ VERB _ MOD
SUB -> "la tarde y la mañana" | "la vasta noche" | "el espejo"
VERB -> "es" | "no sabe" | "algo me han dicho" | "no copia"
MOD -> "bajo" _ OBJ | "Lejos de " _ OBJ | "no es más" _ OBJ _ "can" | "antes" | "pero" _ SENTENCE
OBJ -> "lo que yo siento" | "alba" | "una fragancia"
_ -> " "
